<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.punchcard">
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/sun"
        android:label="@string/app_name"
        android:roundIcon="@drawable/sun"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RecordActivity" />
        <activity android:name=".NewrecordActivity" />
        <activity android:name=".CalendarActivity" />
        <activity android:name=".ClockActivity" />
        <activity android:name=".TreeActivity" />
        <activity
            android:name=".NewnotesActivity"
            android:label="@string/title_activity_newnotes"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>